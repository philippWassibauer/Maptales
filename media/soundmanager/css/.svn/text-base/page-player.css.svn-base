ul.playlist.shiny li.sm2_paused,
ul.playlist.shiny li.sm2_playing {
 background-image:url(../image/top-highlight.png);
 background-repeat:repeat-x;
 background-position:0px -1px;
 _background-image:none; /* can't be bothered with IE 6. */
}

#audioMetadataBlock .loadingBg{
    width: 100%;
    height: 2px;
    background-color: #FFFFFF;
    position: relative;
    top: 4px;
}

#audioMetadataBlock .loading{
    background-color: #D5D6D7;
    width: 100%;
    height: 100%;
}

#audioMetadataBlock .position{
    position: absolute;
    width: 0px;
    height: 2px;
    left: 0px;
    top: 0px;
    background-color: #FFFFFF;
    overflow: visible;
    background-position: 100% -4px;
}

#audioMetadataBlock .knob{
    background-image: url(../button.png);
    height: 10px;
    width: 10px;
    margin-right: -5px;
    margin-top: -4px;
    position: absolute;
    right: 0px;
    cursor: pointer;
}

.volumeControl{
    position: absolute;
    width: 100%;
    top: 50px;
    height: 10px;
}

.volumeControl .down{
    background-image: url(../volume1.png);
    border: 0px solid #FFFFFF;
    left: 0px;
    position: absolute;
    margin-left: 10px;
    height: 15px;
    width: 16px;
}

.volumeControl .up{
    background-image: url(../volume2.png);
    border: 0px solid #FFFFFF;
    right: 0px;
    left: auto;
    position: absolute;
    height: 15px;
    width: 19px;
}

.volumeControl .volumeBar{
   height: 2px;
   background-color: #D5D6D7;
   position: absolute;
   left: 28px;
   width: 40px;
   top: 6px;
}

.volumeBar .volumelevelBar{
   background-color: #FFFFFF;
   height: 2px;
   width: 50%;
   position: absolute;
}

.volumeControl .knob{
    background-image: url(../button.png);
    height: 10px;
    width: 10px;
    margin-right: -5px;
    margin-top: -4px;
    position: absolute;
    right: 0px;
    cursor: pointer;
}

.volumeControl .ui-slider-handle{
    height: 2px !important;
    width: 7px !important;
    margin-left: -3px !important;
    border: 1px solid #333333;
    border:1px solid #D3D3D3;
    color:#555555;
    font-weight:normal;
    outline-color:-moz-use-text-color;
    outline-style:none;
    outline-width:medium;
    top:-0.3em;
    position:absolute;
    padding:0.3em 0 0.1em;
    -moz-border-radius-bottomleft:4px;
    -moz-border-radius-bottomright:4px;
    -moz-border-radius-topleft:4px;
    -moz-border-radius-topright:4px;
    overflow: hidden;
}




ul.playlist li .editpanel{
    position: absolute;
    right: 5px;
    text-align: right;
}

ul.playlist li .editpanel a:hover,
ul.playlist li .editpanel a{
    display: inline;
    text-decoration: underline !important;
}